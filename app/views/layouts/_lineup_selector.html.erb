<% if !lineup_practice_members.empty? %>
	<table class="selector_table">
    <tr>
      <th align='left' colspan='5'><h1>Lineup</h1></th>
    </tr>
		<% character_group = '' %>
    <tr>
      <% counter = 0 %>
			<% character_group = '' %>
      <% lineup_practice_members.all.each do |practice_member| %>
        <!-- if counter get to expected number, create a new row so these wrap -->
        <% if counter >= 5 || character_group != practice_member.last_name.first.upcase %>
					<% character_group = practice_member.last_name.first.upcase %>
          </tr>
          <tr>
          <% counter = 0 %>
        <% end %>
        <td style="width: 1.5in;" class="selector_cell selector_cell_text_six_character" onmousedown="window.location.href='/practice_room/<%= practice_member.id %>/visit';"><%= truncate(practice_member.full_name, :length => 30) %></td>
        <% counter = counter + 1 %>
      <% end %>
    </tr>
  </table>

<% end %>
